@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>            
        </div>
    </section>
}
<h1>My Sample Assets!</h1>
<h3>HTML5 Video</h3>
<video autoplay="autoplay" width="400" height="300" loop>
    <source src="Assets/Azure_Intro.mp4" />
</video>
<h3>Large Image</h3>
<div>
    <img src="Assets/Picture.png" alt="Picture" />
</div>
